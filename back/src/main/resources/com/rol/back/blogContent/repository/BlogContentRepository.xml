<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rol.back.blogContent.repository.BlogContentRepository">
    <select id="selectTrold301" resultType="com.rol.back.blogContent.bean.repository.SelectTrold301Returns">
        SELECT
            T1.object_id
            , T1.registered_date
            , T1.title
            , T1.content
        FROM
            TROLD301 T1
        WHERE
            T1.object_id= #{objectId}
    </select>
</mapper>